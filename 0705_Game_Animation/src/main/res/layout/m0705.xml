<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m0705_r001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back11"
    android:padding="@dimen/p15"
    android:clipChildren="false">

    <TextView
        android:id="@+id/m0705_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@color/Yellow"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/m0705_t001"
        android:textColor="@color/Navy"
        android:textSize="32sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/m0705_t002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/p30"
        android:layout_marginTop="@dimen/p15"
        android:background="@color/Aqua"
        android:text="@string/m0705_t002"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/m0705_t003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/m0705_t002"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/p30"
        android:background="@color/Aqua"
        android:text="@string/m0705_t003"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/m0705_b001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t003"
        android:layout_alignEnd="@+id/m0705_t003"
        android:layout_marginTop="@dimen/p15"
        android:layout_marginEnd="@dimen/p15"
        android:background="@drawable/circle_shape"
        android:src="@drawable/scissors"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b001"
        android:layout_alignEnd="@+id/m0705_b001"
        android:layout_marginTop="@dimen/p15"
        android:background="#00000000"
        android:src="@drawable/stone"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b002"
        android:layout_alignEnd="@+id/m0705_b002"
        android:layout_marginTop="@dimen/p15"
        android:background="#00000000"
        android:src="@drawable/net"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageSwitcher
        android:id="@+id/m0705_c001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/m0705_t002"
        android:layout_alignTop="@+id/m0705_b002"
        android:layout_marginStart="@dimen/p15"
        android:background="@drawable/ring"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/scissor"
        android:textColor="@color/Black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        app:srcCompat="@drawable/scissors" />

    <TextView
        android:id="@+id/m0705_s001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b003"
        android:layout_marginTop="@dimen/p15"
        android:background="@color/Gray"
        android:gravity="start|center_horizontal"
        android:text="@string/m0705_s001"
        android:textColor="@color/Red"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0705_f000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_s001"
        android:layout_marginTop="@dimen/p15"
        android:background="@color/Silver"
        android:gravity="start|center_horizontal"
        android:text="@string/m0705_f000"
        android:textColor="@color/Black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/p15"
        android:layout_marginBottom="@dimen/p15"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />
</RelativeLayout>